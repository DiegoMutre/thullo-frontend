{
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:import/typescript",
    "next/core-web-vitals"
  ],
  "rules": {
    "import/order": [
      "warn",
      {
        "newlines-between": "always",
        "groups": ["builtin", "external"]
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "typescript": true,
      "node": true
    }
  }
}
